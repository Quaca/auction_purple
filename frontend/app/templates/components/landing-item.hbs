            </li>
        </ul>
    </div>
    <div class="col-lg-8 item">
        <div class="row">
            <div class="item-description col-lg-6">
                <h1>{{model.name}}</h1>
                <h5>Start from - ${{model.startingPrice}}</h5>
                <p>{{model.description}}</p>
            </div>
            <div class="item-image col-lg-6">
                <img src="{{model.photos.[0].photoPath}}" alt="/" width="300px">
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-sm-12">
                <button class="custom-button w-20">BID NOW {{fa-icon 'angle-right' size="lg"}}</button>
            </div>
        </div>
    </div>
